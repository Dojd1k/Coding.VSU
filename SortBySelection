k = 0
while k < len(list) - 1:  # -1, Last elemet not trade
    m = k  # at 'm' min value
    i = k + 1
    while i < len(list):
        if list[i] < list[m]:
            m = i
        i = i + 1
    t = list[k]
    list[k] = list[m]
    list[m] = t
    k = k + 1
    print(list)
    
    list=(10,234,23,12)
